# Server configuration
SERVER_PORT=3000
SERVER_IP=127.0.0.1

# PostgreSQL database configuration
POSTGRES_HOST=your_postgres_host
POSTGRES_PORT=5432
POSTGRES_USER=your_postgres_user
POSTGRES_DB=your_postgres_database
POSTGRES_PASSWORD=your_postgres_password

# JWT Authentication secret
AUTH_JWT_SECRET=your_jwt_secret

# Client application settings
CLIENT_RP_ID=your_client_rp_id
CLIENT_URL=https://yourclienturl.com

# Email service credentials
EMAIL_USER=your_email@example.com
EMAIL_APP_PASSWORD=your_email_app_password
EMAIL_APPLICATION_NAME=YourAppName

# Blockchain RPC URLs
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/your_infura_project_id
BASE_RPC_URL=https://your_base_rpc_url
STELLAR_RPC_ENDPOINT=https://your_stellar_rpc_endpoint

# Dojah KYC service keys
DOJAH_APP_ID=your_dojah_app_id
DOJAH_AUTH_PUBLIC_KEY=your_dojah_auth_public_key

# QWallet API keys
QWALLET_SECRET_KEY=your_qwallet_secret_key

# YellowCard (YC) secret keys per environment
DEV_YC_SECRET_KEY=your_dev_yc_secret_key
TESTING_YC_SECRET_KEY=your_testing_yc_secret_key
TESTNET_YC_SECRET_KEY=your_testnet_yc_secret_key
STAGING_YC_SECRET_KEY=your_staging_yc_secret_key
QA_YC_SECRET_KEY=your_qa_yc_secret_key
PROD_YC_SECRET_KEY=your_prod_yc_secret_key

# YellowCard public key
YC_PUBLIC_KEY=your_yc_public_key

# KYC encryption keys per environment
DEV_KYC_ENCRYPTION_KEY=your_dev_kyc_encryption_key
TESTING_KYC_ENCRYPTION_KEY=your_testing_kyc_encryption_key
TESTNET_KYC_ENCRYPTION_KEY=your_testnet_kyc_encryption_key
STAGING_KYC_ENCRYPTION_KEY=your_staging_kyc_encryption_key
QA_KYC_ENCRYPTION_KEY=your_qa_kyc_encryption_key
PROD_KYC_ENCRYPTION_KEY=your_prod_kyc_encryption_key

# Alchemy API keys per environment
DEV_ALCHEMY_API=your_dev_alchemy_api_key
TESTING_ALCHEMY_API=your_testing_alchemy_api_key
TESTNET_ALCHEMY_API=your_testnet_alchemy_api_key
STAGING_ALCHEMY_API=your_staging_alchemy_api_key
QA_ALCHEMY_API=your_qa_alchemy_api_key
PROD_ALCHEMY_API=your_prod_alchemy_api_key

# IP Info token
IPINFO_TOKEN=your_ipinfo_token

